@model MvcCv.Models.Entity.TblHobbies
@{
    ViewBag.Title = "UpdateHobby";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Hobi Düzenleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Hobi Başlık")
    @Html.TextBoxFor(x => x.Description1, new { @class = "form-control" })
    <br />
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Description2, new { @class = "form-control" })
    <br />
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>





